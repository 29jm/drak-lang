def mod(a, b) {
    if a < b {
        return a;
    }

    return mod(a - b, b);
}

def print(n) {
    if n < 10 {
        print_char(48 + n);
        return 0;
    }

    print_char(48 + mod(n, 10));
    print(n / 10);
}

def mul(a, b) {
    return a * b;
}

def main() {
    print(432);
    return 32 + mod(mul(3, 15), mod(23, mul(29, 1)));
}