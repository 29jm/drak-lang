def factorial(n) {
    if n == 1 {
        return 1;
    }

    return n * factorial(n - 1);
}

def fibonacci(n) {
    a = 0;
    b = 1;
    i = 0;

    while i < n {
        c = a;
        a = b;
        b = a + c;
        i = i + 1;
    }

    return b;
}

def test_math() {
    n = 100*(3+(15+3*6))/((3*(4-2)-1)*10);

    return n == 72;
}

print(factorial(10));
print(fibonacci(10));

print(test_math())