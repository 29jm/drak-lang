def mod(a: int, b: int): int {
    if a < b {
        return a;
    }

    return mod(a - b, b);
}

def count_digits(n: int): int {
    if n < 10 {
        return 1;
    }

    return 1 + count_digits(n / 10);
}

def nth_power(a: int, n: int): int {
    if n == 0 {
        return 1;
    }

    return a * nth_power(a, n-1);
}

def get_nth_digit(num: int, n: int): int {
    return mod(num / nth_power(10, n), 10);
}

def print_nth_digit(num: int, n: int): int {
    print_char(48 + get_nth_digit(num, n));
}

def print(n: int): none {
    ndig: int = count_digits(n) - 1;
 
    while ndig != 0
    {
        print_nth_digit(n, ndig);
        ndig = ndig - 1;
    }

    print_nth_digit(n, 0);
    print_char(10);
}

def main(): int {
    print(10405);
    return 32;
}